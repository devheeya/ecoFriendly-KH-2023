<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"https://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>

	<typeAliases>
		<typeAlias type="com.kh.eco.user.model.vo.User" alias="user"/>
		<typeAlias type="com.kh.eco.user.model.vo.Cert" alias="cert"/>
		<typeAlias type="com.kh.eco.user.model.vo.KakaoUser" alias="kakaouser"/>
		<typeAlias type="com.kh.eco.product.model.vo.Product" alias="product"/>
		<typeAlias type="com.kh.eco.product.model.vo.Brand" alias="brand"/>
		<typeAlias type="com.kh.eco.product.model.vo.ProductReview" alias="productReview"/>
		<typeAlias type="com.kh.eco.product.model.vo.ProductOption" alias="option"/>
		<typeAlias type="com.kh.eco.product.model.vo.ProductLike" alias="productLike"/>
		<typeAlias type="com.kh.eco.product.model.vo.Cart" alias="cart"/>
		<typeAlias type="com.kh.eco.product.model.vo.Order" alias="order"/>
		<typeAlias type="com.kh.eco.book.model.vo.Book" alias="book" />
		<typeAlias type="com.kh.eco.book.model.vo.BookReply" alias="bookReply" />
		<typeAlias type="com.kh.eco.book.model.vo.BookReport" alias="bookReport" />
		<typeAlias type="com.kh.eco.book.model.vo.BookReportReply" alias="bookReportReply" />
		<typeAlias type="com.kh.eco.book.model.vo.ReportBlack" alias="adminReportBlack" />
		<typeAlias type="com.kh.eco.book.model.vo.ReportReplyBlack" alias="adminReportReplyBlack" />
		<typeAlias type="com.kh.eco.product.model.vo.ApproveRequest" alias="approveRequest" />
		<typeAlias type="com.kh.eco.challenge.model.vo.Challenge" alias="challenge" />
		<typeAlias type="com.kh.eco.event.model.vo.Event" alias="event"/>
	</typeAliases>

	<mappers>
		<mapper resource="/mapper/challenge/challenge-mapper.xml" />
		<mapper resource="/mapper/event/event-mapper.xml"/>
		<mapper resource="/mapper/user/user-mapper.xml"/>
		<mapper resource ="/mapper/product/product-mapper.xml"/>
  		<mapper resource="/mapper/book/book-mapper.xml" />
	</mappers>
  
</configuration>